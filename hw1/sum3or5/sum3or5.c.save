// function sum3or5
// this function returns the sum of integers less than N
// that are multiples of 3 or 5


#include <stdio.h>
int main(char *args[]){
	int n;
	int counter;
	int i;
	printf("Enter a number: ");
	scanf("%d", &n);
	for (i=1;i<n;i++){
	if (i%3 ==  0 ){
		counter += i;}
	if (i%5 == 0){
		counter += i;}
}
	printf("%d\n", counter);

return 0;


}
